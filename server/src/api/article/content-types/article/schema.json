    },
    "externalId": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "views": {
      "type": "integer"
    },
    "description": {
      "type": "text"
    },
    "image": {
      "type": "string"
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "articles"
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "mappedBy": "articles"
    },
    "content": {
      "type": "richtext"
    },
    "externalPublishDate": {
      "type": "datetime"
    }
